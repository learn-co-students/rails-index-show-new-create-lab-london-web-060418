<h3>Coupon Form</h3>

  <%= form_tag coupons_path(@coupon) do %>
    <label>Cupon Code:</label><br>
    <input type="text" id="coupon_code" name="coupon[coupon_code]"><br>

    <label>Store:</label><br>
    <input id="store" name="coupon[store]"><br>

    <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
    <input type="submit" id="Submit Coupon" value="Submit Cupon">
  <% end %>
